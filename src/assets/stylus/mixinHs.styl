  .splitBar{
    background: rgba(26,27,31,0.40);
  }
  .main-wrap .title{
    border-bottom: 1px solid $border-color-dark;
  }
  .main-wrap .subSelect{
    border-bottom: 1px dashed $border-color-dark;
  }
  .bread-nav{
    border-bottom: 1px solid $border-color-dark!important; 
    background:$background-color-theme-dark!important;
    span{
      color: $font-color-themesec-dark!important;
    }
    span.on{
      color: $font-color-theme-dark!important;
    }
  }
  .btn-group {
    span{
      color: $font-color-theme-dark;
    }
    .btn.active{
      background: $color-main-dark
    }
    .btn{
      color: $font-color-themesec-dark;
    }
  }
  // 地图
  .regionArea{
    background: $background-color-theme-dark!important
    border-bottom:1px solid #1a1b1f;
    box-shadow: none!important;
  }
  #regMap,#mainMap {
    .sline{
      color: #131313;
    }
    .searchArea .circleType span{
      color:  #9D9EA0
      &.active{
        color: #DEDEDF;
      }
    }
  }
  // 地图1弹窗
  #tabWrap{ 
    .tabClick{
      background: rgba(26,27,31,0.90);
      color: $font-color-themesec-dark
      .active0{
          color: $font-color-theme-dark;
      }
    }
    .tabCon{
      background: rgba(26,27,31,0.90);
    }
    .lineBorder{
      border-bottom: 1px solid rgba(255,255,255,0.10);
      background: rgba(26,27,31,0.90);
    }

  }
  .info_num{
    color: $font-color-theme-dark;
  }
  .info_title{
    color: $font-color-themesec-dark
  }
  .info_content{
    color: $font-color-theme-dark;
  }
  .lift_readMore{
    color: $font-color-theme-dark;
  }
  .shadowMarker{
    background: radial-gradient(#4d4f56, rgba(80, 81, 93, 0.31), rgba(48, 51, 55, 0.25), rgb(44, 45, 51))
  }
  .arrowImg4{
    background url("assets/images/hs/mapIcon/arrow4Dark.png") no-repeat;
  }
  .arrowImg3{
    background url("assets/images/hs/mapIcon/arrow3Dark.png") no-repeat;
  }
  .arrowImg2{
    background url("assets/images/hs/mapIcon/arrow2Dark.png") no-repeat;
  }
  .arrowImg1{
    background url("assets/images/hs/mapIcon/arrow1Dark.png") no-repeat;
  }
  .arrowImg0{
    background url("assets/images/hs/mapIcon/arrow0Dark.png") no-repeat;
  }
  .arrowImg{
    background-size: 100% auto;
  }
  // 地图2弹窗
  .map3InfoWindow {
    .map2WindowClass{
      background: rgba(26,27,31,0.90);
    }
    .info_num{
      color: $font-color-theme-dark;
    }
    .info_title{
      color: $font-color-themesec-dark
    }
    .info_content{
      color: $font-color-theme-dark;
    }
    .lift_readMore{
      color: $font-color-theme-dark;
    }
    .arrowImg{
      width: 40px;
      height:40px;
    }
    .arrowImg5{
      background url("assets/images/hs/mapIcon/arrow5Dark.png") no-repeat -5px -5px;
      background-size: 100% auto;
    }
  }
  .main-wrap .blueBtn{
    background-color: $color-main-dark!important;
  }
  .main-wrap .whiteBtn{
    border: 1px solid $border-color-dark;
    color: $font-color-theme-dark;
    background:transparent;
  }
  #mission .openBtn{
    color: $color-main-dark
