  .s_de_details .tie{
    color: $font-color-themesec-dark!important;
  }
  #staffDetails .s_de_department{
    color: $font-color-themesec-dark;
  }
  #staffDetails .open{
    color: $color-main-dark
  }
  #mission .missionTable .missionTr{
    border-bottom: 1px solid $border-color-dark;
  }
  #mission .missionTable .lift{
    color: $font-color-theme-dark;
  }
  // 提示框
  .search-container {
    .llcb-search-tips{
      background: #313238!important;
      box-shadow: 0 0 10px 0 #1A1B1F!important;
    }
    .search-tip-box {
      h4{
        color: $color-white-dark!important;
      }
      p{
        color: $font-color-themesec-dark!important;
      }
      &:hover{
        background: rgba(159,185,247,0.10)!important;
      }
    }
    .on{
      background: rgba(159,185,247,0.10)!important;
    }
