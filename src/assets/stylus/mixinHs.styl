/*深色版；1*/

.theme2 {
  .ant-layout{
    background-color: $background-color-themesec-dark!important
  }
  .main-wrap{
    color: $font-color-theme-dark;
  }
  .main-wrap .panel{
    -webkit-box-shadow: none!important
    box-shadow: none!important
  }
  .the-header {
    background-color: $background-color-theme-dark!important
    box-shadow: 0 1px 4px 0 #191919!important;
  }
  
  .main-wrap .panel {
    background: $background-color-theme-dark
  }
  .splitBar{
    background: rgba(26,27,31,0.40);
  }
  .main-wrap .title{
    border-bottom: 1px solid $border-color-dark;
  }
  .main-wrap .subSelect{
    border-bottom: 1px dashed $border-color-dark;
  }
  .bread-nav{
    border-bottom: 1px solid $border-color-dark!important; 
    background:$background-color-theme-dark!important;
    span{
      color: $font-color-themesec-dark!important;
    }
    span.on{
      color: $font-color-theme-dark!important;
    }
  }
  .btn-group {
    span{
      color: $font-color-theme-dark;
    }
    .btn.active{
      background: $color-main-dark
    }
    .btn{
      color: $font-color-themesec-dark;
    }
  }
  // 地图
  .regionArea{
    background: $background-color-theme-dark!important
    border-bottom:1px solid #000;
    box-shadow: none!important;
  }
  #regMap,#mainMap {
    .sline{
      color: #131313;
    }
    .searchArea .circleType span{
      color:  #9D9EA0
      &.active{
        color: #DEDEDF;
      }
    }
  }
  // 地图1弹窗
  #tabWrap{ 
    .tabClick{
      background: rgba(26,27,31,0.90);
      color: $font-color-themesec-dark
      .active0{
          color: $font-color-theme-dark;
      }
    }
    .tabCon{
      background: rgba(26,27,31,0.90);
    }
    .lineBorder{
      border-bottom: 1px solid rgba(255,255,255,0.10);
      background: rgba(26,27,31,0.90);
    }

  }
  .info_num{
    color: $font-color-theme-dark;
  }
  .info_title{
    color: $font-color-themesec-dark
  }
  .info_content{
    color: $font-color-theme-dark;
  }
  .lift_readMore{
    color: $font-color-theme-dark;
  }
  .shadowMarker{
    background: radial-gradient(rgba(17, 20, 24, 0.95), rgba(1, 13, 25, 0.85), rgba(3, 14, 26, 0.25), rgba(33, 4, 4, 0));
  }
  // 地图2弹窗
  .map3InfoWindow {
    .map2WindowClass{
      background: rgba(26,27,31,0.90);
    }
    .info_num{
      color: $font-color-theme-dark;
    }
    .info_title{
      color: $font-color-themesec-dark
    }
    .info_content{
      color: $font-color-theme-dark;
    }
    .lift_readMore{
      color: $font-color-theme-dark;
    }
  }
  .main-wrap .blueBtn{
    background-color: $color-main-dark;
  }
  .main-wrap .whiteBtn{
    border: 1px solid $border-color-dark;
    color: $font-color-theme-dark;
    background:transparent;
  }
  #mission .openBtn{
    color: $color-main-dark
  }
  .logoCollapse{
    background-color: #2E3138!important;
  }
  #missionDetail .missionOrder{
    background-color: $background-color-theme-dark
  }
  .s_de_details .tie{
    color: $font-color-themesec-dark!important;
  }
  #staffDetails .s_de_department{
    color: $font-color-themesec-dark;
  }
  #staffDetails .open{
    color: $color-main-dark
  }
  #mission .missionTable .missionTr{
    border-bottom: 1px solid $border-color-dark;
  }
  #mission .missionTable .lift{
    color: $font-color-theme-dark;
  }

}

  